{
    "compilerOptions": {
        "esModuleInterop": true,
        "types": [
            "node"
        ],
        "lib": [
            "dom",
            "esnext"
        ],
        "module": "commonjs",
        "outDir": "./dist",
        "strict": true,
        "target": "ES6",
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "experimentalDecorators": true,
    }
}